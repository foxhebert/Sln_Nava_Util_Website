<body class="">
    <div class="page">
        <div class="flex-fill">

            <div class="header py-4">
                <div class="container">
                    <div class="d-flex">
                        <a class="header-brand" href="">
                            @if (objUsuario.empresa.Equals("tecflex"))
                            {
                                <img src="~/images/vector-logo-tecflex2.svg" class="header-brand-img" alt="Tecflex logo">
                            }
                            else
                            {
                                <img src="~/images/vector-logo-codbar.svg" class="header-brand-img" alt="CodBar logo">
                            }

                        </a>
                        <div class="d-flex order-lg-2 ml-auto">

                            <div class="dropleft">
                                <a href="#" class="nav-link pr-0 leading-none" data-toggle="dropdown">
                                    <span class="avatar" style="background-image: url(../../images/user.png)"></span>
                                    <span class="ml-2 d-none d-lg-block">
                                        <span class="text-default">@objUsuario.nomusu</span>
                                        <small class="text-muted d-block mt-1">@objUsuario.Email</small>
                                    </span>
                                </a>
                                <div class="dropdown-menu dropdown-menu-right dropdown-menu-arrow">

                                    <a class="dropdown-item" href="@Url.Action("closeSessionNava","Login")">
                                        <i class="dropdown-icon fe fe-log-out"></i> Cerrar Sesion
                                    </a>
                                </div>
                            </div>
                        </div>
                        <a href="#" class="header-toggler d-lg-none ml-3 ml-lg-0" data-toggle="collapse" data-target="#headerMenuCollapse">
                            <span class="header-toggler-icon"></span>
                        </a>
                    </div>
                </div>
            </div>

            <!-- Navigation View -->
            <div class="sticky-top">
                <div class="header collapse d-lg-flex p-0" id="headerMenuCollapse">
                    <div class="container">

                        <div class="row align-items-center">
                            <div class="col-lg-3 ml-auto">
                                <form class="input-icon my-3 my-lg-0">
                                    <input type="search" class="form-control header-search" placeholder="Search&hellip;" tabindex="1" hidden>
                                    @*<div class="input-icon-addon">
                                            <i class="fe fe-search"></i>
                                        </div>*@
                                </form>
                                @*<div class="dropdown">
                                        <a href="#" class="nav-link pr-0 leading-none" data-toggle="dropdown">
                                            <span class="avatar" style="background-image: url(../../images/user.png)"></span>
                                            <span class="ml-2 d-none d-lg-block">
                                                <span class="text-default">@objUsuario.nomusu</span>
                                                <small class="text-muted d-block mt-1">@objUsuario.Email</small>
                                            </span>
                                        </a>
                                        <div class="dropdown-menu dropdown-menu-right dropdown-menu-arrow">
                                            <a class="dropdown-item" href="@Url.Action("closeSessionNava","Login")">
                                                <i class="dropdown-icon fe fe-log-out"></i> Cerrar Sesion
                                            </a>
                                        </div>
                                    </div>*@

                            </div>
                            <div class="col-lg order-lg-first">
                                <ul class="nav nav-tabs border-0 flex-column flex-lg-row">
                                    @*<li class="nav-item">
                                        <a href="javascript:void(0)" class="nav-link" data-toggle="dropdown"><i class="fe fe-home"></i> Pedido</a>
                                        <div class="dropdown-menu dropdown-menu-arrow">*@
                                    <a href="@Url.Action("ConsultaPedido","Pedido")" class="nav-link"><i class="fa fa-clipboard"></i> Pedido</a>
                                    @*</div>
                                        </li>*@
                                    <li class="nav-item">
                                        <a href="javascript:void(0)" class="nav-link" data-toggle="dropdown"><i class="fa fa-file-text"></i> Guía</a>
                                        <div class="dropdown-menu dropdown-menu-arrow">
                                            <a href="@Url.Action("ConsultarGuia","Guia")" class="dropdown-item ">Consulta de Guías</a>
                                        </div>
                                    </li>
                                    <li class="nav-item dropdown">
                                        <a href="javascript:void(0)" class="nav-link" data-toggle="dropdown"><i class="fa fa-file-text"></i>  Stock</a>  @*class="fe fe-calendar" HGM*@ 
                                        <div class="dropdown-menu dropdown-menu-arrow">
                                            <a href="@Url.Action("ConsultaStock","Stock")" class="dropdown-item ">Consulta de Stock</a>
                                        </div>
                                    </li>
                                    <li class="nav-item dropdown">
                                        <a href="javascript:void(0)" class="nav-link" data-toggle="dropdown"><i class="fa fa-group"></i> Cliente</a>
                                        <div class="dropdown-menu dropdown-menu-arrow">
                                            <a href="@Url.Action("ConsultaCliente","Cliente")" class="dropdown-item ">Consultar Cliente</a>
                                        </div>
                                    </li>
                                    <!--HGM Añadido-->
                                    <li class="nav-item dropdown">
                                        <a href="javascript:void(0)" class="nav-link" data-toggle="dropdown"><i class="fa fa-file-text"></i> Cotización</a>
                                        <div class="dropdown-menu dropdown-menu-arrow">
                                            <a href="@Url.Action("GenerarCotizacion", "Cotizacion")" class="dropdown-item ">Generar Cotización</a>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- fin Navigation View -->

            <div class="" style="margin-top:10px">
                <!--class="my-3 my-md-5"-->
                <div class="container">
                    @RenderBody()
                </div>
            </div>
