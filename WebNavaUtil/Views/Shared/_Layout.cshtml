    @{
        Usuario objUsuario = (Usuario)Session["NavaUsert"];
    }

    <!-- custom css -->
    @if (objUsuario.empresa.Equals("tecflex"))
    {
        <link rel="stylesheet" href="~/Content/assets/css/custom.css">
    }
    else
    {
        <link rel="stylesheet" href="~/Content/assets/css/custom2.css">
